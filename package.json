{
  "name": "marie",
  "displayName": "MARIE",
  "version": "1.0.1",
  "description": "MARIE language support for VS Code",
  "scripts": {
    "build": "node scripts/build.js",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/markypython/marie.git"
  },
  "keywords": [
    "vscode",
    "language"
  ],
  "author": "Mark Skelton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markypython/marie/issues"
  },
  "homepage": "https://github.com/markypython/marie#readme",
  "publisher": "markskelton",
  "engines": {
    "vscode": "^1.12.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "marie",
        "extensions": [
          ".mas"
        ]
      }
    ],
    "grammars": [
      {
        "language": "marie",
        "scopeName": "source.marie",
        "path": "./lib/syntaxes/marie.tmGrammar.json"
      }
    ]
  },
  "badges": [
    {
      "href": "https://travis-ci.com/markypython/vscode-marie",
      "url": "https://travis-ci.com/markypython/vscode-marie.svg?branch=master",
      "description": "TravisCI build status"
    },
    {
      "href": "https://marketplace.visualstudio.com/items?itemName=markskelton.marie",
      "url": "https://img.shields.io/vscode-marketplace/v/markskelton.marie.svg",
      "description": "Visual Studio Marketplace version"
    }
  ],
  "devDependencies": {},
  "dependencies": {
    "yaml": "^1.4.0"
  }
}
